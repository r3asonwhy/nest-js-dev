{
  "name": "backend-template",
  "version": "0.0.1",
  "scripts": {
    "build": "APP_ENV=production nest build",
    "start": "APP_ENV=development node dist/main",
    "start:dev": "cross-env APP_ENV=development nest start --watch",
    "start:debug": "cross-env APP_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "test": "jest"
  },
  "dependencies": {
    "@faker-js/faker": "^9.2.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^10.4.8",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.8",
    "@nestjs/sequelize": "^10.0.1",
    "@nestjs/swagger": "^8.0.7",
    "amazon-cognito-identity-js": "^6.3.12",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "handlebars": "^4.7.8",
    "hbs": "^4.2.0",
    "jwks-rsa": "^3.1.0",
    "mysql2": "^3.11.4",
    "nestjs-i18n": "^10.5.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/hapi__joi": "^17.1.15",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "mkdirp": "^3.0.1",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
