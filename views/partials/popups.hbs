  {{#if isAuction}}
  <!-- auction-bid popup  -->
  <div class="popup-content" data-rel="confirm-bid">
    <div class="layer-close"></div>
    <div class="popup-container">
      <form class="popup-align default-popup">
        <div class="default-popup__image">
          <img src="/img/icons/popup/payment-cards.svg" alt="" loading="lazy">
        </div>
        <div class="default-popup__title">
          <div class="title h4">
            Make Offer
          </div>
        </div>
        <div class="default-popup__content">
          <div class="form-block">
            <div class="row">
              <div class="col-12">
                <div class="input-field">
                  <div class="input-label">Enter amount <span class="info-icon">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 21" width="20" height="21" fill="none">
                        <path stroke="#949494" stroke-linecap="round" stroke-width="1.5"
                          d="M10 18.833a8.333 8.333 0 1 0 0-16.667 8.333 8.333 0 0 0 0 16.667ZM10.008 13.834V10.5M10.008 7.167h-.009">
                        </path>
                      </svg>
                      <span class="info-icon__text">
                        An initial up-front partial payment for the purchase
                      </span>
                    </span></div>
                  <div class="input-field__input-wrap">
                    <div class="input-field__inner-label">AED</div>
                    <input id="auction-bid-input" required class="input group-digits-input-mask"
                      data-trailing-zeroes="3" data-inputmask="'max':99999999999999999" name="totalAmount" type="text"
                      placeholder="1000" required="" inputmode="decimal">
                    <div class="input-error">Please fill in this field</div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="default-popup__buttons">
          <button class="btn btn_secondary close-popup" type="button"><span>Cancel</span></button>
          <button class="btn btn_primary" id="confirm_bid_action" data-id="{{auction.id}}"><span>Confirm</span></button>
        </div>
      </form>
      <button class="btn-close close-popup"></button>
    </div>
  </div>
  <!-- confirm-auction-offer popup  -->
  <div class="popup-content" data-rel="confirm-auction-offer">
    <div class="layer-close"></div>
    <div class="popup-container">
      <div class="popup-align default-popup">
        <div class="default-popup__image">
          <img src="/img/icons/popup/money-question.svg" alt="" loading="lazy">
        </div>
        <div class="default-popup__title">
          <div class="title h4">
            Confirm this offer
          </div>
        </div>
        <div class="default-popup__content">
          <div class="popup-confirm-auction">
            <div class="user-block">
              {{#if highestBidUser.user.avatar}}
              <div class="image-cover image-round">
                <picture>
                  <source id="highest_offer_pic" srcset="{{{imagePath highestBidUser.user.avatar null}}}"
                    type="image/webp">
                  <img id="highest_offer_img" src="{{{imagePath highestBidUser.user.avatar null}}}" alt=""
                    loading="lazy">
                </picture>
              </div>
              {{/if}}
              <div class="title h4" id="highest_offer_name">{{highestBidUser.user.first_name}}
                {{highestBidUser.user.last_name}}</div>
              {{#if highestBidUser.bid}}
              <div class="text-l">Offer: <span class="fw-500" id="highest_offer_bid">{{formatPrice
                  highestBidUser.bid}}</span></div>
              {{else}}
              <div class="text-l">Offer: <span class="fw-500" id="highest_offer_bid">0 AED</span></div>
              {{/if}}
            </div>
          </div>
          <div class="text-m text-center">
            If confirmed, auction will be complited.
          </div>
        </div>
        <div class="default-popup__buttons">
          <button class="btn btn_secondary close-popup" type="button"><span>Cancel</span></button>
          <button class="btn btn_primary auction_payment_button" id="validate_payment"
            data-property-id="{{property.id}}" data-offer-id="{{highestBidUser.id}}"
            data-order-type="confirm-booking"><span>Confirm</span></button>
        </div>
      </div>
      <button class="btn-close close-popup"></button>
    </div>
  </div>
  <!-- confirm-leave-auction popup  -->
  <div class="popup-content" data-rel="confirm-leave-auction">
    <div class="layer-close"></div>
    <div class="popup-container">
      <div class="popup-align default-popup">
        <div class="default-popup__image">
          <img src="/img/icons/popup/close.svg" alt="" loading="lazy">
        </div>
        <div class="default-popup__title">
          <div class="title h4">
            Confirm leave auction
          </div>
          <div class="text-m">If confirmed, your existing offer will be canceled and payment will be automatically
            refunded</div>
        </div>
        <div class="default-popup__buttons">
          <button class="btn btn_secondary close-popup"><span>Cancel</span></button>
          <button class="btn btn_primary" id="leave_auction_action"
            data-id="{{auction.id}}"><span>Confirm</span></button>
        </div>
      </div>
      <button class="btn-close close-popup"></button>
    </div>
  </div>
  <!-- confirm-stop-auction popup  -->
  <div class="popup-content" data-rel="confirm-stop-auction">
    <div class="layer-close"></div>
    <div class="popup-container">
      <div class="popup-align default-popup">
        <div class="default-popup__image">
          <img src="/img/icons/popup/close.svg" alt="" loading="lazy">
        </div>
        <div class="default-popup__title">
          <div class="title h4">
            Confirm Cancel Auction
          </div>
          <div class="text-m">If confirmed, all the existing offers for your property will be canceled</div>
        </div>
        <div class="default-popup__buttons">
          <button class="btn btn_secondary close-popup"><span>Cancel</span></button>
          <button class="btn btn_primary" id="stop_auction_action"
            data-id="{{auction.id}}"><span>Confirm</span></button>
        </div>
      </div>
      <button class="btn-close close-popup"></button>
    </div>
  </div>
  {{/if}}
</div>