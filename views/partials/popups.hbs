                          <span>1</span>
                        </label>
                        <label class="check-btn ch-box">
                          <input type="checkbox" name="bedrooms" value="2" {{#ifCond
                            queryRentFilters.bedroom_number 'includes' "2" }}checked{{/ifCond}}>
                          <span>2</span>
                        </label>
                        <label class="check-btn ch-box">
                          <input type="checkbox" name="bedrooms" value="3" {{#ifCond
