                        <div class="ch-box-wrap ch-box-grid ch-box-grid_3">
                          {{#each propertyAmenities.show}}
                          <label class="check-btn ch-box">
                            <input data-id="{{id}}" {{#ifCond ../queryRentFilters.amenities 'includes'
                              id}}checked{{/ifCond}} type="checkbox" value="{{name}}" name="Amenities">
                            <span>{{name}}</span>
                          </label>
                          {{/each}}
                        </div>
                        {{#if propertyAmenities.hide}}
                        <div class="more-content">
                          <div class="ch-box-wrap ch-box-grid ch-box-grid_3">
